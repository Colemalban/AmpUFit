<h1>Today we are doing <%=@workout.name%></h1>
<div class="container">
	<div class="row">
		<div class="panel-group" id="accordion">
		<%=@workout_categories.each do |var|%>
			<div class="panel panel-default">
				<div class="panel-heading">
				<h4 class="panel-title">
		<a data-toggle="collapse" data-parent="#accordion" href="#collapseOne"><%=var.name%></a></h4>
			</div>
			<div id="collapseOne" class="panel-collapse collapse in">

	<div class="panel-body">
		<table class="table">
			<%= Workout.where(workout_category_id:var.id).each do |workout|%>
			<tr> <td> <span>workout.name</span> </td> </tr>
			<%end%>
			
			</table>
	</div>
	</div>
	<%end%>
</div>


                    </div>
                </div>
